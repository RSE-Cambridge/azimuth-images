[Unit]
Description=
Wants=network.target
After=network-online.target
ConditionPathExists=!/var/lib/webconsole-init.done

[Service]
Type=oneshot
RemainAfterExit=true
ExecStart=/usr/bin/webconsole-init
ExecStartPost=touch /var/lib/webconsole-init.done

[Install]
WantedBy=multi-user.target default.target
